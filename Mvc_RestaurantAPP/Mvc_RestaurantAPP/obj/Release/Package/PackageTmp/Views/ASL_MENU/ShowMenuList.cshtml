@model IEnumerable<Mvc_RestaurantAPP.Models.ASL_MENU>

@{
    ViewBag.Title = "ShowMenuList";

    var LoggedUserTp = HttpContext.Current.Session["LoggedUserType"].ToString();
    var LoggedUserStatus = HttpContext.Current.Session["LoggedUserStatus"].ToString();

    if (LoggedUserTp == "AslSuperadmin" && LoggedUserStatus == "A")
    {

    }
    else
    {
        Response.Redirect(Url.Action("Index", "Login"));
    }
}

<div class="container">
    <div class="row">
        <div class="col-md-2"></div>
        <div class="col-md-8">
            <div class="panel panel-info">
                <div class="panel-heading text-center">
                    <h2> Show all Menu Information</h2>
                </div>
            </div>
        </div>
    </div>
</div>


<div class="table-responsive">
    <table class="table table-striped">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.MODULEID)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MENUTP)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MENUID)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MENUNM)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ACTIONNAME)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CONTROLLERNAME)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.USERPC)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.INSUSERID)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.INSTIME)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.INSIPNO)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.UPDUSERID)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.UPDTIME)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.UPDIPNO)
            </th>
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.MODULEID)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.MENUTP)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.MENUID)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.MENUNM)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.ACTIONNAME)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.CONTROLLERNAME)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.USERPC)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.INSUSERID)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.INSTIME)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.INSIPNO)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.UPDUSERID)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.UPDTIME)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.UPDIPNO)
                </td>
            </tr>
        }

    </table>
</div>
